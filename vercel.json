{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "installCommand": "pnpm install --frozen-lockfile",
  "buildCommand": "cd ../.. && pnpm turbo run build --filter=web",
  "headers": [
    {
      "source": "/icons/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*).ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/manifest.json",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 10 * * *"
    },
    {
      "path": "/api/cron/discover",
      "schedule": "0 */6 * * *"
    },
    {
      "path": "/api/cron/send-digests?frequency=daily",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/send-digests?frequency=weekly",
      "schedule": "0 9 * * 1"
    },
    {
      "path": "/api/cron/send-digests?frequency=monthly",
      "schedule": "0 9 1 * *"
    },
    {
      "path": "/api/cron/send-notifications",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/cron/process-jobs",
      "schedule": "* * * * *"
    },
    {
      "path": "/api/cron/e2ee-cleanup",
      "schedule": "0 3 * * *"
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "claude-insider.com"
        }
      ],
      "destination": "https://www.claudeinsider.com/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.claude-insider.com"
        }
      ],
      "destination": "https://www.claudeinsider.com/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "claudeinsider.com"
        }
      ],
      "destination": "https://www.claudeinsider.com/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "claude-insider.vercel.app"
        }
      ],
      "destination": "https://www.claudeinsider.com/:path*",
      "permanent": true
    }
  ]
}
